{
  "name": "cra-typescript-eslint-prettier-example",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "hooks-for-redux": "^2.0.1",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.19.16",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.2",
    "sass": "^1.32.8",
    "typescript": "^4.1.3",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "build:development": "env-cmd --file .env.development npm run build",
    "eject": "react-scripts eject",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "npm run format:check && eslint .",
    "start": "react-scripts start",
    "start:production": "env-cmd --file .env.production npm start",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
